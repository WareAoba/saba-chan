{
  "name": "discord-bot",
  "version": "0.1.0",
  "description": "Discord Bot for game server management",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "discord.js": "^14.0.0",
    "dotenv": "^16.0.0",
    "i18next": "^25.8.4"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "nodemon": "^3.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "utils/**/*.js",
      "index.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/test/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  }
}
